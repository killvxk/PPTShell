    <addonModule addonId="BasicQuestionViewer" id="BasicQuestionViewer" left="0" top="0" width="100%" height="100%" right="0" bottom="0" isVisible="true" isLocked="false">
      <layout type="LTWH">
        <left relative="" property="left"/>
        <right relative="" property="right"/>
        <top relative="" property="top"/>
        <bottom relative="" property="bottom"/>
      </layout>
      <properties>
        <property name="id" type="string" value="${QuestionId}"/>
        <property name="type_code" type="string" value="${TypeCode}"/>
        <property name="type_name" type="string" value="${TypeName}"/>
        <property name="url" type="string" value="${ref-path}/${EduType}/esp/questions/${QuestionId}.pkg/item.xml"/>
      </properties>
    </addonModule>
  </modules>
</page>
