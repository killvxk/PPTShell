<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<Window size="400, 200">
  <Font id="1" name="微软雅黑" size="14" bold="false" />
  <Font id="2" name="微软雅黑" size="14" bold="true" />
  <VerticalLayout name="MessageBox" bkcolor="#00000000" >
    <Control />
    <VerticalLayout name = "MessageBoxHeight" height="200">
      <HorizontalLayout>
        <Control />
        <VerticalLayout width="400" >
          <HorizontalLayout height="50" inset="10,0,0,0" childpadding="10" bkcolor="#FF000000">
            <Container width="56" bkimage="win-icr\\progressbar\\logo.png" />
            <Label font="2" text="推屏" name="title" textcolor="#FFFFFFFF" endellipsis="true" />
          </HorizontalLayout>
     
          <VerticalLayout  height="100" bkcolor="#FFFFFFFF"  leftbordersize="2"  rightbordersize="3" bordercolor="#FF000000">
            <Control height="20" />
            <Label name="info" text="正在推屏：0 / 5"  font="1" height="40" padding="20,0,0,0" />
            <Progress name="progress" bkimage="file='win-icr\\AttendClass\\status_bar_bg.png' corner='10,4,10,4'" foreimage="file='win-icr\\AttendClass\\status_bar_fg.png' corner='10,4,10,4'" max="100"  height="20" padding="15,0,15,0"/>
          </VerticalLayout>
          
          <HorizontalLayout bkcolor="#FFFFFFFF" leftbordersize="2"  rightbordersize="3"  bottombordersize="3" bordercolor="#FF000000">
            <Control />
           <Button name="close" font="2" textcolor="#FFFFFFFF" text="取 消" normalimage="win-icr\\progressbar\\btn_sure_default.png" hotimage="win-icr\\progressbar\\btn_sure_hover.png" pushedimage="win-icr\\progressbar\\btn_apply_default.png" width="100" height="35" />
            <Control />
          </HorizontalLayout>
        </VerticalLayout>
        <Control />
      </HorizontalLayout>
    </VerticalLayout>
    <Control />
  </VerticalLayout>
</Window>
